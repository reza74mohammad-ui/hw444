   "source": [
    "ref_path = r\"E:\\AIhw1\\Untitled.jpeg\"\n",
    "input_path = r\"E:\\AIhw1\\.idea\\1.png\"\n",
    "\n",
    "# خواندن تصاویر\n",
    "img_ref = imread(ref_path)\n",
    "img_input = imread(input_path)"
   ]
  },
  {
   "cell_type": "code",
