   "source": [
    "input_images = [r\"E:\\AIhw1\\.idea\\1.png\",r\"E:\\AIhw1\\Untitled.jpeg\"]\n",
    "results = []\n",
    "\n",
    "for path in input_images:\n",
    "    img = imread(path)\n",
    "    if img.ndim == 3:\n",
    "        img = np.mean(img, axis=2)\n",
    "    img = img[:min_shape[0], :min_shape[1]]  # برش به اندازه تصویر مرجع\n",
    "    mse_val = np.mean((img_ref_gray - img)**2)\n",
    "    results.append({\"Image\": path, \"MSE\": mse_val})\n",
    "\n",
    "df_results = pd.DataFrame(results)\n",
    "print(df_results)"
   ]
