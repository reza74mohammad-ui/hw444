   "id": "73b46c11",
   "metadata": {},
   "outputs": [],
   "source": [
    "if img_ref.ndim == 3:\n",
    "    img_ref_gray = np.mean(img_ref, axis=2)\n",
    "else:\n",
    "    img_ref_gray = img_ref\n",
    "\n",
    "if img_input.ndim == 3:\n",
    "    img_input_gray = np.mean(img_input, axis=2)\n",
    "else:\n",
    "    img_input_gray = img_input"
   ]
